! START ROUTER SALON DE FIESTAS
ena
conf t
hostname R-SalonFiestas
no ip domain-lookup
service password-enc
enable secret class

line con 0
password cisco
login
logging sync

line vty 0 4
login
logging sync

int s0/1/0
ip add 10.0.4.177 255.255.255.252
ip nat inside
no shut

int g0/0
ip add 10.0.4.166 255.255.255.248
ip nat inside
no shut

int g0/1.10
encapsulation dot1Q 10
ip add 10.0.4.158 255.255.255.240
ip nat inside

int g0/1.20
encapsulation dot1Q 20
ip add 10.0.3.254 255.255.252.0
ip nat inside

int g0/1.30
encapsulation dot1Q 30
ip add 10.0.4.126 255.255.255.128
ip nat inside

int g0/1
no shut

router rip
ver 2
net 10.0.0.0
net 10.0.4.0
net 10.0.4.144
net 10.0.4.160
net 10.0.4.168
net 10.0.4.176
passive-int g0/0
passive-int g0/1

ip dhcp excluded-address 10.0.4.158
ip dhcp excluded-address 10.0.3.254
ip dhcp excluded-address 10.0.4.126

ip dhcp pool Camerinos
net 10.0.4.144 255.255.255.240
default-router 10.0.4.158

ip dhcp pool Invitados
net 10.0.0.0 255.255.252.0
default-router 10.0.3.254

ip dhcp pool Prensa
net 10.0.4.0 255.255.255.128
default-router 10.0.4.126

! Listas de control de acceso
access-list 110 deny tcp any host 10.0.4.130 eq 80
access-list 110 deny tcp any host 10.0.4.130 eq 20
access-list 110 deny tcp any host 10.0.4.130 eq 21
access-list 110 deny ip host 10.0.0.2 host 54.173.144.172
access-list 110 permit ip any any
int g0/1.10
ip access-group 110 in
int g0/1.20
ip access-group 110 in
int g0/1.30
ip access-group 110 in

! END ROUTER SALON DE FIESTAS

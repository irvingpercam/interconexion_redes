! START ROUTER FAMILIA FERNANDEZ
ena
conf t
hostname R-FamFernandez
no ip domain-lookup
service password-enc
enable secret class

line con 0
password cisco
login
logging sync

line vty 0 4
login
logging sync
exit

int s0/0/1
ip add 16.4.64.241 255.255.255.240
ip nat outside
no shut

int s0/0/0
ip add 10.0.4.178 255.255.255.252
ip nat inside
no shut

int g0/1
ip add 10.0.4.142 255.255.255.240
ip nat inside
no shut

ip route 0.0.0.0 0.0.0.0 s0/0/1

router rip
ver 2
net 10.0.4.128
net 10.0.4.176
redist static
passive-int s0/0/1

ip dhcp excluded-address 10.0.4.142

ip dhcp pool R-FamFernandez
net 10.0.4.128 255.255.255.240
default-router 10.0.4.142

! NAT Estatico
ip nat inside source static 10.0.4.161 132.255.255.241

! NAT Dinamico
! Empieza a hacer traducciones a partir de la .242
ip nat pool natPool 132.255.255.242 132.255.255.255 netmask 255.255.255.240
! Permite todo lo que llegue al router que tenga la direccion 10.-.-.-
access-list 10 permit 10.0.0.0 0.255.255.255
! Activar access-list
ip nat inside source list 10 pool natPool overload

! END ROUTER FAMILIA FERNANDEZ
